var myHeaders = new Headers();
myHeaders.append("Cookie", "session=it9oeu2q9divcpjfdvglcqu643");

var formdata = new FormData();
formdata.append("voice", "rh-oleg");
formdata.append("text", "Здравствуйте");

var requestOptions = {
  method: 'POST',
  headers: myHeaders,
  body: formdata,
  redirect: 'follow'
};

fetch("https://voxworker.com/ru/ajax/convert", requestOptions)
  .then(response => response.text())
  .then(result => console.log(result))
  .catch(error => console.log('error', error));